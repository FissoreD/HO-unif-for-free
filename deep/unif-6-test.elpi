type compile-terms 
  list (triple diagnostic try6.fo.fm try6.fo.fm) -> 
  list (triple diagnostic try6.ho.tm try6.ho.tm) -> 
  try6.comp.links ->
  try6.ho.subst -> try6.ho.subst ->
  o.
  
compile-terms [] [] [] H H.
compile-terms [triple D S1 S2|Src] [triple D T1 T2|PS] LS H0 H3 :-
  std.assert! (try6.comp.comp S1 T1 [] L1 H0 H1) "comp s1",
  std.assert! (try6.comp.comp S2 T2 L1 L2 H1 H2) "comp s2",
  compile-terms Src PS Links2 H2 H3,
  std.append L2 Links2 LS.

type prolog 
  list (triple diagnostic try6.ho.tm try6.ho.tm) -> 
  try6.comp.links -> try6.comp.links ->
  try6.ho.subst -> try6.ho.subst ->
  diagnostic ->
  o.
prolog [] L L H H ok :- print "end proggram\n".
prolog [triple ok L R|Us] Links Links2 H H3 D :- std.do! [
  print "## prolog unif step" {try6.ho.pp {try6.ho.deref H L}} "=" {try6.ho.pp {try6.ho.deref H R}},
  try6.ho.unif L R H H1,
  print "## After unif subst is" {try6.ho.ppsubst H1},
  print "## Links are" {try6.comp.pplinks Links},
  printC "solve links?",
  try6.comp.solve-links-fixpoint Links Links1 H1 H2,
  print "## After solve links, links are" {try6.comp.pplinks Links1},
  print "## After solve links, subst is" {try6.ho.ppsubst H2},
  ],
  !,
  printC "next problem?",
  prolog Us Links1 Links2 H2 H3 D.
prolog [_|_] L L H H (error "xx").
prolog [triple (error "unif") L R|_] Links Links H H1 (error "stop") :-
  print "## unif (error)" L "=" R,
  if (try6.ho.unif L R H H1)
     (print "## After unif" L "=" R "subst is" {try6.ho.ppsubst H1},
      print "## Links are" {try6.comp.pplinks Links},
      printC "solve links?",
      not(try6.comp.solve-links-fixpoint Links _ H1 _))
     (H1 = H).

type decompile-subst
  try6.comp.links -> 
  try6.ho.subst ->
  try6.fo.subst ->
  try6.fo.subst ->
  o.
decompile-subst Links H F F1 :-
  try6.comp.decompl Links H F F1.

